{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "resolveJsonModule": false,
    "sourceMap": true,
    "composite": true,
    "declaration": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "removeComments": false
  }
}