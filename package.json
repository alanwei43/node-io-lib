{
  "name": "node-io-lib",
  "version": "0.0.7",
  "description": "NodeJS IO Library",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage --config=jest.config.ts",
    "build": "rm -rf ./dist && tsc",
    "lint": "eslint ./ --ext .ts",
    "pub": "tsc && npm publish --access public",
    "cli:tree": "ts-node src/bin/cli.ts tree ./ --deep 5 --dir-exclude 'node_modules|git'"
  },
  "bin": {
    "node-io": "dist/bin/cli.js",
    "node-io-lib": "dist/bin/cli.js"
  },
  "keywords": [
    "nodejs",
    "io",
    "file"
  ],
  "author": "Alan Wei",
  "license": "ISC",
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanwei43/node-io-lib.git"
  },
  "bugs": {
    "url": "https://github.com/alanwei43/node-io-lib/issues"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "node-fetch": "^3.1.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=10"
  }
}
